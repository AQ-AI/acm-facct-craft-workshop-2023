{
  "name": "single-page-markdown-website",
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "author": "Yuan Qing Lim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/single-page-markdown-website.git",
    "directory": "packages/single-page-markdown-website"
  },
  "files": [
    "build",
    "lib"
  ],
  "scripts": {
    "build": "yarn run clean && tsc",
    "clean": "rimraf '*.log' .nyc_output coverage lib",
    "fix": "eslint --fix 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "prepublishOnly": "yarn run build",
    "start": "yarn run clean && chokidar --command 'cd sandbox && ts-node ../src/cli.ts \"*.md\" --output index.html' --initial --silent 'build/**/*' 'src/**/*.ts'",
    "test": "tap 'src/**/__tests__/**/*.ts' --coverage-report html --coverage-report text --jobs-auto --no-browser --reporter terse",
    "watch": "chokidar --command 'yarn run build' --initial --silent 'build/**/*' 'src/**/*.ts'"
  },
  "dependencies": {
    "@yuanqing/cli": "^0.0.7",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "gray-matter": "^4.0.2",
    "html-minifier": "^4.0.0",
    "mdast-util-toc": "^5.1.0",
    "rehype-autolink-headings": "^5.0.1",
    "rehype-highlight": "^4.0.0",
    "rehype-slug": "^4.0.1",
    "rehype-stringify": "^8.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-stringify": "^9.0.0",
    "unified": "^9.2.0"
  }
}
